    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="9.0.0-rc.1.24451.1" />
    <PackageReference Include="Npgsql" Version="8.0.4" />
    <PackageReference Include="Simplify.Web.Json" Version="2.4.0" />
